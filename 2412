% 10. VELOCIDADE TANGECIAL 

% 10.1 MATRIZ DE VELOCIDADE DO ESCOAMENRO UNIVEFORME (alpha: -10° à 15°)

%Para alpha1
u_U1=V*(cos(theta)*cos(alpha1)+sin(theta)*sin(alpha1));

%Para alpha2
u_U2=V*(cos(theta)*cos(alpha2)+sin(theta)*sin(alpha2));

%Para alpha3
u_U3=V*(cos(theta)*cos(alpha3)+sin(theta)*sin(alpha3));

%Para alpha4
u_U4=V*(cos(theta)*cos(alpha4)+sin(theta)*sin(alpha4));

%Para alpha5
u_U5=V*(cos(theta)*cos(alpha5)+sin(theta)*sin(alpha5));

%Para alpha6
u_U6=V*(cos(theta)*cos(alpha6)+sin(theta)*sin(alpha6));

%Para alpha7
u_U7=V*(cos(theta)*cos(alpha7)+sin(theta)*sin(alpha7));

%Para alpha8
u_U8=V*(cos(theta)*cos(alpha8)+sin(theta)*sin(alpha8));

%Para alpha9
u_U9=V*(cos(theta)*cos(alpha9)+sin(theta)*sin(alpha9));

%Para alpha10
u_U10=V*(cos(theta)*cos(alpha10)+sin(theta)*sin(alpha10));

%Para alpha11
u_U11=V*(cos(theta)*cos(alpha11)+sin(theta)*sin(alpha11));

%Para alpha12
u_U12=V*(cos(theta)*cos(alpha12)+sin(theta)*sin(alpha12));

%Para alpha13
u_U13=V*(cos(theta)*cos(alpha13)+sin(theta)*sin(alpha13));

%Para alpha14
u_U14=V*(cos(theta)*cos(alpha14)+sin(theta)*sin(alpha14));

%Para alpha15
u_U15=V*(cos(theta)*cos(alpha15)+sin(theta)*sin(alpha15));

%Para alpha16
u_U16=V*(cos(theta)*cos(alpha16)+sin(theta)*sin(alpha16));

%Para alpha17
u_U17=V*(cos(theta)*cos(alpha17)+sin(theta)*sin(alpha17));

%Para alpha18
u_U18=V*(cos(theta)*cos(alpha18)+sin(theta)*sin(alpha18));

%Para alpha19
u_U19=V*(cos(theta)*cos(alpha19)+sin(theta)*sin(alpha19));

%Para alpha20
u_U20=V*(cos(theta)*cos(alpha20)+sin(theta)*sin(alpha20));

%Para alpha21
u_U21=V*(cos(theta)*cos(alpha21)+sin(theta)*sin(alpha21));

%Para alpha22
u_U22=V*(cos(theta)*cos(alpha22)+sin(theta)*sin(alpha22));

%Para alpha23
u_U23=V*(cos(theta)*cos(alpha23)+sin(theta)*sin(alpha23));

%Para alpha24
u_U24=V*(cos(theta)*cos(alpha24)+sin(theta)*sin(alpha24));

%Para alpha25
u_U25=V*(cos(theta)*cos(alpha25)+sin(theta)*sin(alpha25));

%Para alpha26
u_U26=V*(cos(theta)*cos(alpha26)+sin(theta)*sin(alpha26));


% 10.2 MATRIZ DE COEFICIENTES DE VELOCIDADE DEVIDO AS FONTES
U_F(1,:)=u_f(1,:)*cos(theta(1,1))+v_f(1,:)*sin(theta(1,1));
U_F(2,:)=u_f(2,:)*cos(theta(2,1))+v_f(2,:)*sin(theta(2,1));
U_F(3,:)=u_f(3,:)*cos(theta(3,1))+v_f(3,:)*sin(theta(3,1));
U_F(4,:)=u_f(4,:)*cos(theta(4,1))+v_f(4,:)*sin(theta(4,1));
U_F(5,:)=u_f(5,:)*cos(theta(5,1))+v_f(5,:)*sin(theta(5,1));
U_F(6,:)=u_f(6,:)*cos(theta(6,1))+v_f(6,:)*sin(theta(6,1));
U_F(7,:)=u_f(7,:)*cos(theta(7,1))+v_f(7,:)*sin(theta(7,1));
U_F(8,:)=u_f(8,:)*cos(theta(8,1))+v_f(8,:)*sin(theta(8,1));
U_F(9,:)=u_f(9,:)*cos(theta(9,1))+v_f(9,:)*sin(theta(9,1));
U_F(10,:)=u_f(10,:)*cos(theta(10,1))+v_f(10,:)*sin(theta(10,1));
U_F(11,:)=u_f(11,:)*cos(theta(11,1))+v_f(11,:)*sin(theta(11,1));
U_F(12,:)=u_f(12,:)*cos(theta(12,1))+v_f(12,:)*sin(theta(12,1));
U_F(13,:)=u_f(13,:)*cos(theta(13,1))+v_f(13,:)*sin(theta(13,1));
U_F(14,:)=u_f(14,:)*cos(theta(14,1))+v_f(14,:)*sin(theta(14,1));
U_F(15,:)=u_f(15,:)*cos(theta(15,1))+v_f(15,:)*sin(theta(15,1));
U_F(16,:)=u_f(16,:)*cos(theta(16,1))+v_f(16,:)*sin(theta(16,1));
U_F(17,:)=u_f(17,:)*cos(theta(17,1))+v_f(17,:)*sin(theta(17,1));
U_F(18,:)=u_f(18,:)*cos(theta(18,1))+v_f(18,:)*sin(theta(18,1));
U_F(19,:)=u_f(19,:)*cos(theta(19,1))+v_f(19,:)*sin(theta(19,1));
U_F(20,:)=u_f(20,:)*cos(theta(20,1))+v_f(20,:)*sin(theta(20,1));
U_F(21,:)=u_f(21,:)*cos(theta(21,1))+v_f(21,:)*sin(theta(21,1));
U_F(22,:)=u_f(22,:)*cos(theta(22,1))+v_f(22,:)*sin(theta(22,1));
U_F(23,:)=u_f(23,:)*cos(theta(23,1))+v_f(23,:)*sin(theta(23,1));
U_F(24,:)=u_f(24,:)*cos(theta(24,1))+v_f(24,:)*sin(theta(24,1));
U_F(25,:)=u_f(25,:)*cos(theta(25,1))+v_f(25,:)*sin(theta(25,1));
U_F(26,:)=u_f(26,:)*cos(theta(26,1))+v_f(26,:)*sin(theta(26,1));
U_F(27,:)=u_f(27,:)*cos(theta(27,1))+v_f(27,:)*sin(theta(27,1));
U_F(28,:)=u_f(28,:)*cos(theta(28,1))+v_f(28,:)*sin(theta(28,1));
U_F(29,:)=u_f(29,:)*cos(theta(29,1))+v_f(29,:)*sin(theta(29,1));
U_F(30,:)=u_f(30,:)*cos(theta(30,1))+v_f(30,:)*sin(theta(30,1));
U_F(31,:)=u_f(31,:)*cos(theta(31,1))+v_f(31,:)*sin(theta(31,1));
U_F(32,:)=u_f(32,:)*cos(theta(32,1))+v_f(32,:)*sin(theta(32,1));
U_F(33,:)=u_f(33,:)*cos(theta(33,1))+v_f(33,:)*sin(theta(33,1));
U_F(34,:)=u_f(34,:)*cos(theta(34,1))+v_f(34,:)*sin(theta(34,1));
u_F=real(U_F);

% 10.3 MATRIZ DE COEFICIENTES DEVIDO OS VÓRTICES
U_V(1,1)= sum(u_v(1,:))*cos(theta(1,1))+sum(v_v(1,:))*sin(theta(1,1));
U_V(2,1)= sum(u_v(2,:))*cos(theta(2,1))+sum(v_v(2,:))*sin(theta(2,1));
U_V(3,1)= sum(u_v(3,:))*cos(theta(3,1))+sum(v_v(3,:))*sin(theta(3,1));
U_V(4,1)= sum(u_v(4,:))*cos(theta(4,1))+sum(v_v(4,:))*sin(theta(4,1));
U_V(5,1)= sum(u_v(5,:))*cos(theta(5,1))+sum(v_v(5,:))*sin(theta(5,1));
U_V(6,1)= sum(u_v(6,:))*cos(theta(6,1))+sum(v_v(6,:))*sin(theta(6,1));
U_V(7,1)= sum(u_v(7,:))*cos(theta(7,1))+sum(v_v(7,:))*sin(theta(7,1));
U_V(8,1)= sum(u_v(8,:))*cos(theta(8,1))+sum(v_v(8,:))*sin(theta(8,1));
U_V(9,1)= sum(u_v(9,:))*cos(theta(9,1))+sum(v_v(9,:))*sin(theta(9,1));
U_V(10,1)= sum(u_v(10,:))*cos(theta(10,1))+sum(v_v(10,:))*sin(theta(10,1));
U_V(11,1)= sum(u_v(11,:))*cos(theta(11,1))+sum(v_v(11,:))*sin(theta(11,1));
U_V(12,1)= sum(u_v(12,:))*cos(theta(12,1))+sum(v_v(12,:))*sin(theta(12,1));
U_V(13,1)= sum(u_v(13,:))*cos(theta(13,1))+sum(v_v(13,:))*sin(theta(13,1));
U_V(14,1)= sum(u_v(14,:))*cos(theta(14,1))+sum(v_v(14,:))*sin(theta(14,1));
U_V(15,1)= sum(u_v(15,:))*cos(theta(15,1))+sum(v_v(15,:))*sin(theta(15,1));
U_V(16,1)= sum(u_v(16,:))*cos(theta(16,1))+sum(v_v(16,:))*sin(theta(16,1));
U_V(17,1)= sum(u_v(17,:))*cos(theta(17,1))+sum(v_v(17,:))*sin(theta(17,1));
U_V(18,1)= sum(u_v(18,:))*cos(theta(18,1))+sum(v_v(18,:))*sin(theta(18,1));
U_V(19,1)= sum(u_v(19,:))*cos(theta(19,1))+sum(v_v(19,:))*sin(theta(19,1));
U_V(20,1)= sum(u_v(20,:))*cos(theta(20,1))+sum(v_v(20,:))*sin(theta(20,1));
U_V(21,1)= sum(u_v(21,:))*cos(theta(21,1))+sum(v_v(21,:))*sin(theta(21,1));
U_V(22,1)= sum(u_v(22,:))*cos(theta(22,1))+sum(v_v(22,:))*sin(theta(22,1));
U_V(23,1)= sum(u_v(23,:))*cos(theta(23,1))+sum(v_v(23,:))*sin(theta(23,1));
U_V(24,1)= sum(u_v(24,:))*cos(theta(24,1))+sum(v_v(24,:))*sin(theta(24,1));
U_V(25,1)= sum(u_v(25,:))*cos(theta(25,1))+sum(v_v(25,:))*sin(theta(25,1));
U_V(26,1)= sum(u_v(26,:))*cos(theta(26,1))+sum(v_v(26,:))*sin(theta(26,1));
U_V(27,1)= sum(u_v(27,:))*cos(theta(27,1))+sum(v_v(27,:))*sin(theta(27,1));
U_V(28,1)= sum(u_v(28,:))*cos(theta(28,1))+sum(v_v(28,:))*sin(theta(28,1));
U_V(29,1)= sum(u_v(29,:))*cos(theta(29,1))+sum(v_v(29,:))*sin(theta(29,1));
U_V(30,1)= sum(u_v(30,:))*cos(theta(30,1))+sum(v_v(30,:))*sin(theta(30,1));
U_V(31,1)= sum(u_v(31,:))*cos(theta(31,1))+sum(v_v(31,:))*sin(theta(31,1));
U_V(32,1)= sum(u_v(32,:))*cos(theta(32,1))+sum(v_v(32,:))*sin(theta(32,1));
U_V(33,1)= sum(u_v(33,:))*cos(theta(33,1))+sum(v_v(33,:))*sin(theta(33,1));
U_V(34,1)= sum(u_v(34,:))*cos(theta(34,1))+sum(v_v(34,:))*sin(theta(34,1));
u_V=real(U_V);

% 10.4 VELOCIDADE TOTAL EM CADA PAINEL (alpha: -10° à 15°)

%Para alpha1
U_t1 = u_U1 + (u_F*F1) + (W1*u_V);

%Para alpha2
U_t2 = u_U2 + (u_F*F2) + (W2*u_V);

%Para alpha3
U_t3 = u_U3 + (u_F*F3) + (W3*u_V);

%Para alpha4
U_t4 = u_U4 + (u_F*F4) + (W4*u_V);

%Para alpha5
U_t5 = u_U5 + (u_F*F5) + (W5*u_V);

%Para alpha6
U_t6 = u_U6 + (u_F*F6) + (W6*u_V);

%Para alpha7
U_t7 = u_U7 + (u_F*F7) + (W7*u_V);

%Para alpha8
U_t8 = u_U8 + (u_F*F8) + (W8*u_V);

%Para alpha9
U_t9 = u_U9 + (u_F*F9) + (W9*u_V);

%Para alpha10
U_t10 = u_U10 + (u_F*F10) + (W10*u_V);

%Para alpha11
U_t11 = u_U11 + (u_F*F11) + (W11*u_V);

%Para alpha12
U_t12 = u_U12 + (u_F*F12) + (W12*u_V);

%Para alpha13
U_t13 = u_U13 + (u_F*F13) + (W13*u_V);

%Para alpha14
U_t14 = u_U14 + (u_F*F14) + (W14*u_V);

%Para alpha15
U_t15 = u_U15 + (u_F*F15) + (W15*u_V);

%Para alpha16
U_t16 = u_U16 + (u_F*F16) + (W16*u_V);

%Para alpha17
U_t17 = u_U17 + (u_F*F17) + (W17*u_V);

%Para alpha18
U_t18 = u_U18 + (u_F*F18) + (W18*u_V);

%Para alpha19
U_t19 = u_U19 + (u_F*F19) + (W19*u_V);

%Para alpha20
U_t20 = u_U20 + (u_F*F20) + (W20*u_V);

%Para alpha21
U_t21 = u_U21 + (u_F*F21) + (W21*u_V);

%Para alpha22
U_t22 = u_U22 + (u_F*F22) + (W22*u_V);

%Para alpha23
U_t23 = u_U23 + (u_F*F23) + (W23*u_V);

%Para alpha24
U_t24 = u_U24 + (u_F*F24) + (W24*u_V);

%Para alpha25
U_t25 = u_U25 + (u_F*F25) + (W25*u_V);

%Para alpha26
U_t26 = u_U26 + (u_F*F26) + (W26*u_V);


%__________________________________________________________________________

% 11. COEFICIENTE DE PRESSÃO (aplha: -10° à 15°)

% 11.1 COOERDENADAS DOS PONTOS DE CONTROTE PARA GRÁFICOS DO C_p
X_cin=X_c(1:17,1);      % Coordenadas x dos pontos de controle do intradorso
X_cex=X_c(17:34,1);     % Coordenadas x dos pontos de controle do extradorso

% 11.2 cOEFICIENTE DE PRESSÃO (alpha: -10° à 15°)
% Para alpha1
C_p1=1-((U_t1./V).^2)     % Coeficiente de pressão (Adimensional)
Cp_in1=C_p1(1:17,1);      % Coeficiente de pressão no intradorso
Cp_ex1=C_p1(17:34,1);     % Coeficiente de pressão no extradorso

% Para alpha2
C_p2=1-((U_t2./V).^2)     % Coeficiente de pressão (Adimensional)
Cp_in2=C_p2(1:17,1);      % Coeficiente de pressão no intradorso
Cp_ex2=C_p2(17:34,1);     % Coeficiente de pressão no extradorso

