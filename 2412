% 12. COEFICIENTES AERODINÂMICOS (alpha: -10° à 15°)

% 12.1 COEFICIENTE C_x e C_y 

% Para alpha1
C_y(1,1)=-sum(C_p1.*((X_i1-X_i)/P(1,1)));  
C_x(1,1)=sum(C_p1.*((Y_i1-Y_i)/P(1,1)));

% Para alpha2
C_y(1,2)=-sum(C_p2.*((X_i1-X_i)/P(1,1)));  
C_x(1,2)=sum(C_p2.*((Y_i1-Y_i)/P(1,1)));

% Para alpha3
C_y(1,3)=-sum(C_p3.*((X_i1-X_i)/P(1,1)));  
C_x(1,3)=sum(C_p3.*((Y_i1-Y_i)/P(1,1)));

% Para alpha4
C_y(1,4)=-sum(C_p4.*((X_i1-X_i)/P(1,1)));  
C_x(1,4)=sum(C_p4.*((Y_i1-Y_i)/P(1,1)));

% Para alpha5
