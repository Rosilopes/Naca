A(30,:)=v_f(30,:).*cos(theta(30,1))-u_f(30,:).*sin(theta(30,1));
A(31,:)=v_f(31,:).*cos(theta(31,1))-u_f(31,:).*sin(theta(31,1));
A(32,:)=v_f(32,:).*cos(theta(32,1))-u_f(32,:).*sin(theta(32,1));
A(33,:)=v_f(33,:).*cos(theta(33,1))-u_f(33,:).*sin(theta(33,1));
A(34,:)=v_f(34,:).*cos(theta(34,1))-u_f(34,:).*sin(theta(34,1));

% 7.2 COEFICIENTES DE INFLUÊNCIA  N+1,J
A(35,:)=u_f(1,:)*cos(theta(1,1))+v_f(1,:)*sin(theta(1,1))+u_f(34,:)*cos(theta(34,1))+v_f(34,:)*sin(theta(34,1));

% 7.3 COEFICIENTES DE INFLUÊNCIA  I,N+1
A(1,35)= sum(v_v(1,1:34))*cos(theta(1,1))-sum(u_v(1,1:34))*sin(theta(1,1));
A(2,35)= sum(v_v(2,1:34))*cos(theta(2,1))-sum(u_v(2,1:34))*sin(theta(2,1));
A(3,35)= sum(v_v(3,1:34))*cos(theta(3,1))-sum(u_v(3,1:34))*sin(theta(3,1));
A(4,35)= sum(v_v(4,1:34))*cos(theta(4,1))-sum(u_v(4,1:34))*sin(theta(4,1));
A(5,35)= sum(v_v(5,1:34))*cos(theta(5,1))-sum(u_v(5,1:34))*sin(theta(5,1));
A(6,35)= sum(v_v(6,1:34))*cos(theta(6,1))-sum(u_v(6,1:34))*sin(theta(6,1));
A(7,35)= sum(v_v(7,1:34))*cos(theta(7,1))-sum(u_v(7,1:34))*sin(theta(7,1));
A(8,35)= sum(v_v(8,1:34))*cos(theta(8,1))-sum(u_v(8,1:34))*sin(theta(8,1));
A(9,35)= sum(v_v(9,1:34))*cos(theta(9,1))-sum(u_v(9,1:34))*sin(theta(9,1));
A(10,35)= sum(v_v(10,1:34))*cos(theta(10,1))-sum(u_v(10,1:34))*sin(theta(10,1));
A(11,35)= sum(v_v(11,1:34))*cos(theta(11,1))-sum(u_v(11,1:34))*sin(theta(11,1));
A(12,35)= sum(v_v(12,1:34))*cos(theta(12,1))-sum(u_v(12,1:34))*sin(theta(12,1));
A(13,35)= sum(v_v(13,1:34))*cos(theta(13,1))-sum(u_v(13,1:34))*sin(theta(13,1));
A(14,35)= sum(v_v(14,1:34))*cos(theta(14,1))-sum(u_v(14,1:34))*sin(theta(14,1));
A(15,35)= sum(v_v(15,1:34))*cos(theta(15,1))-sum(u_v(15,1:34))*sin(theta(15,1));
A(16,35)= sum(v_v(16,1:34))*cos(theta(16,1))-sum(u_v(16,1:34))*sin(theta(16,1));
A(17,35)= sum(v_v(17,1:34))*cos(theta(17,1))-sum(u_v(17,1:34))*sin(theta(17,1));
A(18,35)= sum(v_v(18,1:34))*cos(theta(18,1))-sum(u_v(18,1:34))*sin(theta(18,1));
A(19,35)= sum(v_v(19,1:34))*cos(theta(19,1))-sum(u_v(19,1:34))*sin(theta(19,1));
A(20,35)= sum(v_v(20,1:34))*cos(theta(20,1))-sum(u_v(20,1:34))*sin(theta(20,1));
A(21,35)= sum(v_v(21,1:34))*cos(theta(21,1))-sum(u_v(21,1:34))*sin(theta(21,1));
A(22,35)= sum(v_v(22,1:34))*cos(theta(22,1))-sum(u_v(22,1:34))*sin(theta(22,1));
A(23,35)= sum(v_v(23,1:34))*cos(theta(23,1))-sum(u_v(23,1:34))*sin(theta(23,1));
A(24,35)= sum(v_v(24,1:34))*cos(theta(24,1))-sum(u_v(24,1:34))*sin(theta(24,1));
A(25,35)= sum(v_v(25,1:34))*cos(theta(25,1))-sum(u_v(25,1:34))*sin(theta(25,1));
A(26,35)= sum(v_v(26,1:34))*cos(theta(26,1))-sum(u_v(26,1:34))*sin(theta(26,1));
A(27,35)= sum(v_v(27,1:34))*cos(theta(27,1))-sum(u_v(27,1:34))*sin(theta(27,1));
A(28,35)= sum(v_v(28,1:34))*cos(theta(28,1))-sum(u_v(28,1:34))*sin(theta(28,1));
A(29,35)= sum(v_v(29,1:34))*cos(theta(29,1))-sum(u_v(29,1:34))*sin(theta(29,1));
A(30,35)= sum(v_v(30,1:34))*cos(theta(30,1))-sum(u_v(30,1:34))*sin(theta(30,1));
A(31,35)= sum(v_v(31,1:34))*cos(theta(31,1))-sum(u_v(31,1:34))*sin(theta(31,1));
A(32,35)= sum(v_v(32,1:34))*cos(theta(32,1))-sum(u_v(32,1:34))*sin(theta(32,1));
A(33,35)= sum(v_v(33,1:34))*cos(theta(33,1))-sum(u_v(33,1:34))*sin(theta(33,1));
A(34,35)= sum(v_v(34,1:34))*cos(theta(34,1))-sum(u_v(34,1:34))*sin(theta(34,1));

% 7.4 COEFICIENTES DE INFLUÊNCIA N+1,N+1
A(35,35)= sum(u_v(1,:)*cos(theta(1,1))+v_v(1,:)*sin(theta(1,1))+u_v(34,:)*cos(theta(34,1))+v_v(34,:)*sin(theta(34,1)));

%__________________________________________________________________________

% 8. CÁLCULO DA MATRIZ b PARA DETERMINAR AS SINGULARIDADES

% 8.1 MATRIZ DOS COEFICIENTES b (Influencia do escoamento univofrme) (alpha: -10° à 15°)

% Para alpha1 
b1=-V*(cos(theta)*sin(alpha1)-sin(theta)*cos(alpha1));
b1(35,1)=-V*(cos(alpha1)*cos(theta(1,1))+sin(alpha1)*sin(theta(1,1))+cos(alpha1)*cos(theta(34,1))+sin(alpha1)*sin(theta(34,1)));

% Para alpha2 
b2=-V*(cos(theta)*sin(alpha2)-sin(theta)*cos(alpha2));
b2(35,1)=-V*(cos(alpha2)*cos(theta(1,1))+sin(alpha2)*sin(theta(1,1))+cos(alpha2)*cos(theta(34,1))+sin(alpha2)*sin(theta(34,1)));

% Para alpha3 
b3=-V*(cos(theta)*sin(alpha3)-sin(theta)*cos(alpha3));
b3(35,1)=-V*(cos(alpha3)*cos(theta(1,1))+sin(alpha3)*sin(theta(1,1))+cos(alpha3)*cos(theta(34,1))+sin(alpha3)*sin(theta(34,1)));

% Para alpha4 
b4=-V*(cos(theta)*sin(alpha4)-sin(theta)*cos(alpha4));
b4(35,1)=-V*(cos(alpha4)*cos(theta(1,1))+sin(alpha4)*sin(theta(1,1))+cos(alpha4)*cos(theta(34,1))+sin(alpha4)*sin(theta(34,1)));

% Para alpha5 
b5=-V*(cos(theta)*sin(alpha5)-sin(theta)*cos(alpha5));
b5(35,1)=-V*(cos(alpha5)*cos(theta(1,1))+sin(alpha5)*sin(theta(1,1))+cos(alpha5)*cos(theta(34,1))+sin(alpha5)*sin(theta(34,1)));

% Para alpha6 
b6=-V*(cos(theta)*sin(alpha6)-sin(theta)*cos(alpha6));
b6(35,1)=-V*(cos(alpha6)*cos(theta(1,1))+sin(alpha6)*sin(theta(1,1))+cos(alpha6)*cos(theta(34,1))+sin(alpha6)*sin(theta(34,1)));

% Para alpha7 
b7=-V*(cos(theta)*sin(alpha7)-sin(theta)*cos(alpha7));
b7(35,1)=-V*(cos(alpha7)*cos(theta(1,1))+sin(alpha7)*sin(theta(1,1))+cos(alpha7)*cos(theta(34,1))+sin(alpha7)*sin(theta(34,1)));

% Para alpha8 
b8=-V*(cos(theta)*sin(alpha8)-sin(theta)*cos(alpha8));
b8(35,1)=-V*(cos(alpha8)*cos(theta(1,1))+sin(alpha8)*sin(theta(1,1))+cos(alpha8)*cos(theta(34,1))+sin(alpha8)*sin(theta(34,1)));

% Para alpha9 
