clc

%                   UNIVERSIDADE FEDERAL DO PIAUÍ
%                DEPARTAMENTO DE ENGENHARIA MECÂNICA


%       APLICAÇÃO DO MÉTODO DOS PAINÉIS EM ESCOAMENTO DE PERFIS 
%                   AERODINÂMICOS BIDIBENSIONAIS

                        
% PERFIL NACA 2412
% 34 PAINÉIS
% ELEMENTOS DE SINGULARIDADES UTILIZADOS: FONTES E VÓRTICES

% ESTRUTURA DO CÓDIGO
    % 1.  VARIÁVEIS DE ENTRADA 
    % 2.  PERFIL
    % 3.  GEOMETRIA DOS PAINÉIS
    % 4.  DETERMINAÇÃO DOS VETORES (r_i), (r_i+1) E DOS ÂNGULOS BETAS (B)
    % 5.  COMPONENTES DE VELOCIDADES INDUZIDAS PELAS SINGULARIDADES NO SISTEMA LOCAL
    % 6.  COMPONENTES DE VELOCIDADES INDUZIDAS PELAS SINGULARIDADES NO SISTEMA GLOBAL
    % 7.  CÁLCULO DA MATRIZ A PARA DETERMINAR AS SINGULARIDADES
    % 8.  CÁLCULO DA MATRIZ b PARA DETERMINAR AS SINGULARIDADES
    % 9.  CÁLCULO DAS SINGULARIDADES
    % 10. VELOCIDADE TANGECIAL 
    % 11. COEFICIENTE DE PRESSÃO
    % 12. COEFICIENTES AERODINÂMICOS
    % 13. GRÁFICOS

%__________________________________________________________________________

% 1 VARIÁVEIS DE ENTRADA

% 1.1 COORDENADAS CARTESIANAS DO PERFIL (34 PAINÉIS, 35 PONTOS DE FRONTEIRA)
P=[1.0000	-0.00
0.9500	-0.0048
0.9000	-0.0082
0.8000	-0.015
0.7000	-0.0214
0.6000	-0.0276
0.5000	-0.0334
0.4000	-0.038
0.3000	-0.0412
0.2500	-0.0422
0.2000	-0.0423
0.1500	-0.041
0.1000	-0.0375
0.0750	-0.0346
0.0500	-0.0301
0.0250	-0.0227
0.0125	-0.0165
0.0000	0
0.0125	0.0215
0.0250	0.0299
0.0500	0.0413
0.0750	0.0496
0.1000	0.0563
0.1500	0.0661
0.2000	0.0726
0.2500	0.0767
0.3000	0.0788
0.4000	0.078
0.5000	0.0724
0.6000	0.0636
0.7000	0.0518
0.8000	0.0375
0.9000	0.0208
0.9500	0.0114
1.0000	0.00
];

% 1.3 VELOCIDADE DO ESCOAMENTO
V=20                        % Velocidade de corrente livre (m/s)

% 1.4 ÂNGULOS DE ATAQUE (VARIAÇÃO DE -10° À 15°)
a=-10:1:15;                         % Ângulo de ataque (graus)
 
alpha1=(a(1,1)/180)*pi;             % Ângulo de ataque (radianos)  -10°
alpha2=(a(1,2)/180)*pi;             % Ângulo de ataque (radianos)   -9°
alpha3=(a(1,3)/180)*pi;             % Ângulo de ataque (radianos)   -8°
alpha4=(a(1,4)/180)*pi;             % Ângulo de ataque (radianos)   -7°
alpha5=(a(1,5)/180)*pi;             % Ângulo de ataque (radianos)   -6°
alpha6=(a(1,6)/180)*pi;             % Ângulo de ataque (radianos)   -5°
alpha7=(a(1,7)/180)*pi;             % Ângulo de ataque (radianos)   -4°
alpha8=(a(1,8)/180)*pi;             % Ângulo de ataque (radianos)   -3°
alpha9=(a(1,9)/180)*pi;             % Ângulo de ataque (radianos)   -2°
alpha10=(a(1,10)/180)*pi;           % Ângulo de ataque (radianos)   -1°
alpha11=(a(1,11)/180)*pi;           % Ângulo de ataque (radianos)    0°
alpha12=(a(1,12)/180)*pi;           % Ângulo de ataque (radianos)    1°
alpha13=(a(1,13)/180)*pi;           % Ângulo de ataque (radianos)    2°
alpha14=(a(1,14)/180)*pi;           % Ângulo de ataque (radianos)    3°
alpha15=(a(1,15)/180)*pi;           % Ângulo de ataque (radianos)    4°
alpha16=(a(1,16)/180)*pi;           % Ângulo de ataque (radianos)    5°
alpha17=(a(1,17)/180)*pi;           % Ângulo de ataque (radianos)    6°
alpha18=(a(1,18)/180)*pi;           % Ângulo de ataque (radianos)    7°
alpha19=(a(1,19)/180)*pi;           % Ângulo de ataque (radianos)    8°
alpha20=(a(1,20)/180)*pi;           % Ângulo de ataque (radianos)    9°
alpha21=(a(1,21)/180)*pi;           % Ângulo de ataque (radianos)   10°
alpha22=(a(1,22)/180)*pi;           % Ângulo de ataque (radianos)   11°
alpha23=(a(1,23)/180)*pi;           % Ângulo de ataque (radianos)   12°
alpha24=(a(1,24)/180)*pi;           % Ângulo de ataque (radianos)   13°
alpha25=(a(1,25)/180)*pi;           % Ângulo de ataque (radianos)   14°
alpha26=(a(1,26)/180)*pi;           % Ângulo de ataque (radianos)   15°

alpha=(a/180)*pi;

%__________________________________________________________________________

% 2 PERFIL

% 2.1 COORDENADAS DOS PAINÉIS 
X_in=P(1:18,1);      % Coordenada x do intradorso
Y_in=P(1:18,2);      % Cooerdenada y do intradorso
X_ex=P(18:35,1);     % Coordenada x do extradorso
Y_ex=P(18:35,2);     % Cooerdenada y do extradorso

% 2.2 DESENHO DO PERFIL 
figure;plot(X_ex,Y_ex,'b','linewidth',2);hold on; plot(X_in,Y_in,'r','linewidth',2), grid
title('PERFIL NACA 2412')
axis([0 1 -.5 .5])
xlabel('x')
ylabel('y')

%__________________________________________________________________________

% 3. GEOMETRIA DOS PAINÉIS

% 3.1 PONTOS DE FRONTEIRA
X_f=P(:,1);          % Coordenada x dos Pontos de fronteiras 
Y_f=P(:,2);          % Coordenada y dos Pontos de fronteiras
X_i=P(1:34,1);      % Coordenada x do ponto i
X_i1=P(2:35,1);     % Coordenada x do ponto i+1
Y_i=P(1:34,2);      % Coordenada y do ponto i
Y_i1=P(2:35,2);     % Coordenada y do ponto i+1

% 3.2 PONTOS DE CONTROLE (Ponto médio dos painéis)
X_c=(X_i+X_i1)./2;   %Coordenada x dos pontos de controle
Y_c=(Y_i+Y_i1)./2;   %Coordenada y dos pontos de controle

% 3.3 COMPRIMENTO DOS PAINÉIS
L=((X_i1-X_i).^2 + (Y_i1-Y_i).^2).^.5;

% 1.2 ÂNGULO THETA ENTRE OS PAINEIS E O EIXO DAS ABSCISSAS (radianos)
X_n1=P(1:17,1);     % Coordenada xi do intradorso
X_n2=P(2:18,1);     % Coordenada xi+1 do intradorso
X_m1=P(18:34,1);    % Coordenada xi do extradorso
X_m2=P(19:35,1);    % Coordenada xi+1 do exntradorso

Y_n1=P(1:17,2);     % Coordenada yi do intradorso
Y_n2=P(2:18,2);     % Coordenada yi+1 do intradorso
Y_m1=P(18:34,2);    % Coordenada yi do extradorso
Y_m2=P(19:35,2);    % Coordenada yi+1 do extradorso

theta(1:17,1)=atan((Y_n2-Y_n1)./(X_n2-X_n1))+pi     % Ângulo entre os painéis e o eixo x (i=1 à n/2)
theta(18:34,1)=atan((Y_m2-Y_m1)./(X_m2-X_m1))       % Ângulo entre os painéis e o eixo x (i=n/2 à n)
%__________________________________________________________________________

% 4. DETERMINAÇÃO DOS VETORES (r_i), (r_i+1) E DOS ÂNGULOS BETAS (B)

% 4.1 MATRIZ DOS VETORES r_i
r_i(:,1)=sqrt((X_i(1,1)-X_c).^2+(Y_i(1,1)-Y_c).^2);
r_i(:,2)=sqrt((X_i(2,1)-X_c).^2+(Y_i(2,1)-Y_c).^2);
r_i(:,3)=sqrt((X_i(3,1)-X_c).^2+(Y_i(3,1)-Y_c).^2);
r_i(:,4)=sqrt((X_i(4,1)-X_c).^2+(Y_i(4,1)-Y_c).^2);
r_i(:,5)=sqrt((X_i(5,1)-X_c).^2+(Y_i(5,1)-Y_c).^2);
r_i(:,6)=sqrt((X_i(6,1)-X_c).^2+(Y_i(6,1)-Y_c).^2);
r_i(:,7)=sqrt((X_i(7,1)-X_c).^2+(Y_i(7,1)-Y_c).^2);
r_i(:,8)=sqrt((X_i(8,1)-X_c).^2+(Y_i(8,1)-Y_c).^2);
r_i(:,9)=sqrt((X_i(9,1)-X_c).^2+(Y_i(9,1)-Y_c).^2);
r_i(:,10)=sqrt((X_i(10,1)-X_c).^2+(Y_i(10,1)-Y_c).^2);
r_i(:,11)=sqrt((X_i(11,1)-X_c).^2+(Y_i(11,1)-Y_c).^2);
r_i(:,12)=sqrt((X_i(12,1)-X_c).^2+(Y_i(12,1)-Y_c).^2);
r_i(:,13)=sqrt((X_i(13,1)-X_c).^2+(Y_i(13,1)-Y_c).^2);
r_i(:,14)=sqrt((X_i(14,1)-X_c).^2+(Y_i(14,1)-Y_c).^2);
r_i(:,15)=sqrt((X_i(15,1)-X_c).^2+(Y_i(15,1)-Y_c).^2);
r_i(:,16)=sqrt((X_i(16,1)-X_c).^2+(Y_i(16,1)-Y_c).^2);
r_i(:,17)=sqrt((X_i(17,1)-X_c).^2+(Y_i(17,1)-Y_c).^2);
r_i(:,18)=sqrt((X_i(18,1)-X_c).^2+(Y_i(18,1)-Y_c).^2);
r_i(:,19)=sqrt((X_i(19,1)-X_c).^2+(Y_i(19,1)-Y_c).^2);
r_i(:,20)=sqrt((X_i(20,1)-X_c).^2+(Y_i(20,1)-Y_c).^2);
r_i(:,21)=sqrt((X_i(21,1)-X_c).^2+(Y_i(21,1)-Y_c).^2);
r_i(:,22)=sqrt((X_i(22,1)-X_c).^2+(Y_i(22,1)-Y_c).^2);
r_i(:,23)=sqrt((X_i(23,1)-X_c).^2+(Y_i(23,1)-Y_c).^2);
r_i(:,24)=sqrt((X_i(24,1)-X_c).^2+(Y_i(24,1)-Y_c).^2);
r_i(:,25)=sqrt((X_i(25,1)-X_c).^2+(Y_i(25,1)-Y_c).^2);
r_i(:,26)=sqrt((X_i(26,1)-X_c).^2+(Y_i(26,1)-Y_c).^2);
r_i(:,27)=sqrt((X_i(27,1)-X_c).^2+(Y_i(27,1)-Y_c).^2);
r_i(:,28)=sqrt((X_i(28,1)-X_c).^2+(Y_i(28,1)-Y_c).^2);
r_i(:,29)=sqrt((X_i(29,1)-X_c).^2+(Y_i(29,1)-Y_c).^2);
r_i(:,30)=sqrt((X_i(30,1)-X_c).^2+(Y_i(30,1)-Y_c).^2);
r_i(:,31)=sqrt((X_i(31,1)-X_c).^2+(Y_i(31,1)-Y_c).^2);
r_i(:,32)=sqrt((X_i(32,1)-X_c).^2+(Y_i(32,1)-Y_c).^2);
r_i(:,33)=sqrt((X_i(33,1)-X_c).^2+(Y_i(33,1)-Y_c).^2);
r_i(:,34)=sqrt((X_i(34,1)-X_c).^2+(Y_i(34,1)-Y_c).^2);

% 4.2 MATRIZ DOS VETORES r_i+1
r_i1(:,1)=sqrt((X_i1(1,1)-X_c).^2+(Y_i1(1,1)-Y_c).^2);
r_i1(:,2)=sqrt((X_i1(2,1)-X_c).^2+(Y_i1(2,1)-Y_c).^2);
r_i1(:,3)=sqrt((X_i1(3,1)-X_c).^2+(Y_i1(3,1)-Y_c).^2);
r_i1(:,4)=sqrt((X_i1(4,1)-X_c).^2+(Y_i1(4,1)-Y_c).^2);
r_i1(:,5)=sqrt((X_i1(5,1)-X_c).^2+(Y_i1(5,1)-Y_c).^2);
r_i1(:,6)=sqrt((X_i1(6,1)-X_c).^2+(Y_i1(6,1)-Y_c).^2);
r_i1(:,7)=sqrt((X_i1(7,1)-X_c).^2+(Y_i1(7,1)-Y_c).^2);
r_i1(:,8)=sqrt((X_i1(8,1)-X_c).^2+(Y_i1(8,1)-Y_c).^2);
r_i1(:,9)=sqrt((X_i1(9,1)-X_c).^2+(Y_i1(9,1)-Y_c).^2);
r_i1(:,10)=sqrt((X_i1(10,1)-X_c).^2+(Y_i1(10,1)-Y_c).^2);
r_i1(:,11)=sqrt((X_i1(11,1)-X_c).^2+(Y_i1(11,1)-Y_c).^2);
r_i1(:,12)=sqrt((X_i1(12,1)-X_c).^2+(Y_i1(12,1)-Y_c).^2);
r_i1(:,13)=sqrt((X_i1(13,1)-X_c).^2+(Y_i1(13,1)-Y_c).^2);
r_i1(:,14)=sqrt((X_i1(14,1)-X_c).^2+(Y_i1(14,1)-Y_c).^2);
r_i1(:,15)=sqrt((X_i1(15,1)-X_c).^2+(Y_i1(15,1)-Y_c).^2);
r_i1(:,16)=sqrt((X_i1(16,1)-X_c).^2+(Y_i1(16,1)-Y_c).^2);
r_i1(:,17)=sqrt((X_i1(17,1)-X_c).^2+(Y_i1(17,1)-Y_c).^2);
r_i1(:,18)=sqrt((X_i1(18,1)-X_c).^2+(Y_i1(18,1)-Y_c).^2);
r_i1(:,19)=sqrt((X_i1(19,1)-X_c).^2+(Y_i1(19,1)-Y_c).^2);
r_i1(:,20)=sqrt((X_i1(20,1)-X_c).^2+(Y_i1(20,1)-Y_c).^2);
r_i1(:,21)=sqrt((X_i1(21,1)-X_c).^2+(Y_i1(21,1)-Y_c).^2);
r_i1(:,22)=sqrt((X_i1(22,1)-X_c).^2+(Y_i1(22,1)-Y_c).^2);
r_i1(:,23)=sqrt((X_i1(23,1)-X_c).^2+(Y_i1(23,1)-Y_c).^2);
r_i1(:,24)=sqrt((X_i1(24,1)-X_c).^2+(Y_i1(24,1)-Y_c).^2);
r_i1(:,25)=sqrt((X_i1(25,1)-X_c).^2+(Y_i1(25,1)-Y_c).^2);
r_i1(:,26)=sqrt((X_i1(26,1)-X_c).^2+(Y_i1(26,1)-Y_c).^2);
r_i1(:,27)=sqrt((X_i1(27,1)-X_c).^2+(Y_i1(27,1)-Y_c).^2);
r_i1(:,28)=sqrt((X_i1(28,1)-X_c).^2+(Y_i1(28,1)-Y_c).^2);
r_i1(:,29)=sqrt((X_i1(29,1)-X_c).^2+(Y_i1(29,1)-Y_c).^2);
r_i1(:,30)=sqrt((X_i1(30,1)-X_c).^2+(Y_i1(30,1)-Y_c).^2);
r_i1(:,31)=sqrt((X_i1(31,1)-X_c).^2+(Y_i1(31,1)-Y_c).^2);
r_i1(:,32)=sqrt((X_i1(32,1)-X_c).^2+(Y_i1(32,1)-Y_c).^2);
r_i1(:,33)=sqrt((X_i1(33,1)-X_c).^2+(Y_i1(33,1)-Y_c).^2);
r_i1(:,34)=sqrt((X_i1(34,1)-X_c).^2+(Y_i1(34,1)-Y_c).^2);

% 4.3 MATRIZ DOS ÂNGULOS BETAS
B(:,1)=acos((r_i(:,1).^2 +r_i1(:,1).^2-L(1,1).^2)./(2*r_i(:,1).*r_i1(:,1)));
B(:,2)=acos((r_i(:,2).^2 +r_i1(:,2).^2-L(2,1).^2)./(2*r_i(:,2).*r_i1(:,2)));
B(:,3)=acos((r_i(:,3).^2 +r_i1(:,3).^2-L(3,1).^2)./(2*r_i(:,3).*r_i1(:,3)));
B(:,4)=acos((r_i(:,4).^2 +r_i1(:,4).^2-L(4,1).^2)./(2*r_i(:,4).*r_i1(:,4)));
B(:,5)=acos((r_i(:,5).^2 +r_i1(:,5).^2-L(5,1).^2)./(2*r_i(:,5).*r_i1(:,5)));
B(:,6)=acos((r_i(:,6).^2 +r_i1(:,6).^2-L(6,1).^2)./(2*r_i(:,6).*r_i1(:,6)));
B(:,7)=acos((r_i(:,7).^2 +r_i1(:,7).^2-L(7,1).^2)./(2*r_i(:,7).*r_i1(:,7)));
B(:,8)=acos((r_i(:,8).^2 +r_i1(:,8).^2-L(8,1).^2)./(2*r_i(:,8).*r_i1(:,8)));
B(:,9)=acos((r_i(:,9).^2 +r_i1(:,9).^2-L(9,1).^2)./(2*r_i(:,9).*r_i1(:,9)));
B(:,10)=acos((r_i(:,10).^2 +r_i1(:,10).^2-L(10,1).^2)./(2*r_i(:,10).*r_i1(:,10)));
B(:,11)=acos((r_i(:,11).^2 +r_i1(:,11).^2-L(11,1).^2)./(2*r_i(:,11).*r_i1(:,11)));
B(:,12)=acos((r_i(:,12).^2 +r_i1(:,12).^2-L(12,1).^2)./(2*r_i(:,12).*r_i1(:,12)));
B(:,13)=acos((r_i(:,13).^2 +r_i1(:,13).^2-L(13,1).^2)./(2*r_i(:,13).*r_i1(:,13)));
B(:,14)=acos((r_i(:,14).^2 +r_i1(:,14).^2-L(14,1).^2)./(2*r_i(:,14).*r_i1(:,14)));
B(:,15)=acos((r_i(:,15).^2 +r_i1(:,15).^2-L(15,1).^2)./(2*r_i(:,15).*r_i1(:,15)));
B(:,16)=acos((r_i(:,16).^2 +r_i1(:,16).^2-L(16,1).^2)./(2*r_i(:,16).*r_i1(:,16)));
B(:,17)=acos((r_i(:,17).^2 +r_i1(:,17).^2-L(17,1).^2)./(2*r_i(:,17).*r_i1(:,17)));
B(:,18)=acos((r_i(:,18).^2 +r_i1(:,18).^2-L(18,1).^2)./(2*r_i(:,18).*r_i1(:,18)));
B(:,19)=acos((r_i(:,19).^2 +r_i1(:,19).^2-L(19,1).^2)./(2*r_i(:,19).*r_i1(:,19)));
B(:,20)=acos((r_i(:,20).^2 +r_i1(:,20).^2-L(20,1).^2)./(2*r_i(:,20).*r_i1(:,20)));
B(:,21)=acos((r_i(:,21).^2 +r_i1(:,21).^2-L(21,1).^2)./(2*r_i(:,21).*r_i1(:,21)));
B(:,22)=acos((r_i(:,22).^2 +r_i1(:,22).^2-L(22,1).^2)./(2*r_i(:,22).*r_i1(:,22)));
B(:,23)=acos((r_i(:,23).^2 +r_i1(:,23).^2-L(23,1).^2)./(2*r_i(:,23).*r_i1(:,23)));
B(:,24)=acos((r_i(:,24).^2 +r_i1(:,24).^2-L(24,1).^2)./(2*r_i(:,24).*r_i1(:,24)));
B(:,25)=acos((r_i(:,25).^2 +r_i1(:,25).^2-L(25,1).^2)./(2*r_i(:,25).*r_i1(:,25)));
B(:,26)=acos((r_i(:,26).^2 +r_i1(:,26).^2-L(26,1).^2)./(2*r_i(:,26).*r_i1(:,26)));
B(:,27)=acos((r_i(:,27).^2 +r_i1(:,27).^2-L(27,1).^2)./(2*r_i(:,27).*r_i1(:,27)));
B(:,28)=acos((r_i(:,28).^2 +r_i1(:,28).^2-L(28,1).^2)./(2*r_i(:,28).*r_i1(:,28)));
B(:,29)=acos((r_i(:,29).^2 +r_i1(:,29).^2-L(29,1).^2)./(2*r_i(:,29).*r_i1(:,29)));
B(:,30)=acos((r_i(:,30).^2 +r_i1(:,30).^2-L(30,1).^2)./(2*r_i(:,30).*r_i1(:,30)));
B(:,31)=acos((r_i(:,31).^2 +r_i1(:,31).^2-L(31,1).^2)./(2*r_i(:,31).*r_i1(:,31)));
B(:,32)=acos((r_i(:,32).^2 +r_i1(:,32).^2-L(32,1).^2)./(2*r_i(:,32).*r_i1(:,32)));
B(:,33)=acos((r_i(:,33).^2 +r_i1(:,33).^2-L(33,1).^2)./(2*r_i(:,33).*r_i1(:,33)));
B(:,34)=acos((r_i(:,34).^2 +r_i1(:,34).^2-L(34,1).^2)./(2*r_i(:,34).*r_i1(:,34)));

% ___________________________________________________________________________

% 5. COMPONENTES DE VELOCIDADES INDUZIDAS PELAS SINGULARIDADES NO SISTEMA LOCAL

% 5.1 COMPONENTE X DA VELOCIDADE DE  INFLUÊNCIA DAS FONTES  
u_f_l=(1/(2*pi))*log(r_i./r_i1);

% 5.2 COMPONENTE Y DA VELOCIDADE DE  INFLUÊNCIA DAS FONTES  
v_f_l=(1/(2*pi))*B;

% 5.3 COMPONENTE X DA VELOCIDADE DE  INFLUÊNCIA DOS VÓRTICES  
u_v_l=(1/(2*pi))*B;

% 5.4 COMPONENTE Y DA VELOCIDADE DE  INFLUÊNCIA DOS VÓRTICES  
v_v_l=(1/(2*pi))*log(r_i.\r_i1);

%__________________________________________________________________________

% 6. COMPONENTES DE VELOCIDADES INDUZIDAS PELAS SINGULARIDADES NO SISTEMA GLOBAL

% 6.1 COMPONENTES DE VELOCIDADES INDUZIDAS PELAS SINGULARIDADES  
u_f(1,:)= u_f_l(1,:).*cos(theta(1,1))-v_f_l(1,:).*sin(theta(1,1));
u_f(2,:)= u_f_l(2,:).*cos(theta(2,1))-v_f_l(2,:).*sin(theta(2,1));
