% Para alpha16
C_y(1,16)=-sum(C_p16.*((X_i1-X_i)/P(1,1)));  
C_x(1,16)=sum(C_p16.*((Y_i1-Y_i)/P(1,1)));

% Para alpha17
C_y(1,17)=-sum(C_p17.*((X_i1-X_i)/P(1,1)));  
C_x(1,17)=sum(C_p17.*((Y_i1-Y_i)/P(1,1)));

% Para alpha18
C_y(1,18)=-sum(C_p18.*((X_i1-X_i)/P(1,1)));  
C_x(1,18)=sum(C_p18.*((Y_i1-Y_i)/P(1,1)));

% Para alpha19
C_y(1,19)=-sum(C_p19.*((X_i1-X_i)/P(1,1)));  
C_x(1,19)=sum(C_p19.*((Y_i1-Y_i)/P(1,1)));

% Para alpha20
C_y(1,20)=-sum(C_p20.*((X_i1-X_i)/P(1,1)));  
C_x(1,20)=sum(C_p20.*((Y_i1-Y_i)/P(1,1)));

% Para alpha21
C_y(1,21)=-sum(C_p21.*((X_i1-X_i)/P(1,1)));  
C_x(1,21)=sum(C_p21.*((Y_i1-Y_i)/P(1,1)));

% Para alpha22
C_y(1,22)=-sum(C_p22.*((X_i1-X_i)/P(1,1)));  
C_x(1,22)=sum(C_p22.*((Y_i1-Y_i)/P(1,1)));

% Para alpha23
C_y(1,23)=-sum(C_p23.*((X_i1-X_i)/P(1,1)));  
C_x(1,23)=sum(C_p23.*((Y_i1-Y_i)/P(1,1)));

% Para alpha24
C_y(1,24)=-sum(C_p24.*((X_i1-X_i)/P(1,1)));  
C_x(1,24)=sum(C_p24.*((Y_i1-Y_i)/P(1,1)));

% Para alpha25
C_y(1,25)=-sum(C_p25.*((X_i1-X_i)/P(1,1)));  
C_x(1,25)=sum(C_p25.*((Y_i1-Y_i)/P(1,1)));

% Para alpha26
C_y(1,26)=-sum(C_p26.*((X_i1-X_i)/P(1,1)));  
C_x(1,26)=sum(C_p26.*((Y_i1-Y_i)/P(1,1)));


% 12.2 COEFICIENTE DE SUSTENTAÇÃO
C_l=C_y.*cos(alpha)-C_x.*sin(alpha)

% 12.3 COEFICIENTE DE ARRASTO
C_d=C_x.*cos(alpha)+C_y.*sin(alpha)


% 12.4 COEFICIENTES C_mx e C_my (Alpha: -10° à 15°) (alpha: -10° à 15°)

% alpha1
C_mx(1,1)=sum((C_p1.*((Y_i1-Y_i)/P(1,1))).*Y_i);
C_my(1,1)=sum((C_p1.*((X_i1-X_i)/P(1,1))).*(X_i-.5));

% alpha2
C_mx(2,1)=sum((C_p2.*((Y_i1-Y_i)/P(1,1))).*Y_i);
C_my(2,1)=sum((C_p2.*((X_i1-X_i)/P(1,1))).*(X_i-.5));

% alpha3
C_mx(3,1)=sum((C_p3.*((Y_i1-Y_i)/P(1,1))).*Y_i);
C_my(3,1)=sum((C_p3.*((X_i1-X_i)/P(1,1))).*(X_i-.5));

% alpha4
C_mx(4,1)=sum((C_p4.*((Y_i1-Y_i)/P(1,1))).*Y_i);
C_my(4,1)=sum((C_p4.*((X_i1-X_i)/P(1,1))).*(X_i-.5));

% alpha5
C_mx(5,1)=sum((C_p5.*((Y_i1-Y_i)/P(1,1))).*Y_i);
C_my(5,1)=sum((C_p5.*((X_i1-X_i)/P(1,1))).*(X_i-.5));

% alpha6
C_mx(6,1)=sum((C_p6.*((Y_i1-Y_i)/P(1,1))).*Y_i);
C_my(6,1)=sum((C_p6.*((X_i1-X_i)/P(1,1))).*(X_i-.5));

% alpha7
C_mx(7,1)=sum((C_p7.*((Y_i1-Y_i)/P(1,1))).*Y_i);
C_my(7,1)=sum((C_p7.*((X_i1-X_i)/P(1,1))).*(X_i-.5));

% alpha8
C_mx(8,1)=sum((C_p8.*((Y_i1-Y_i)/P(1,1))).*Y_i);
C_my(8,1)=sum((C_p8.*((X_i1-X_i)/P(1,1))).*(X_i-.5));

% alpha9
C_mx(9,1)=sum((C_p9.*((Y_i1-Y_i)/P(1,1))).*Y_i);
C_my(9,1)=sum((C_p9.*((X_i1-X_i)/P(1,1))).*(X_i-.5));

% alpha10
C_mx(10,1)=sum((C_p10.*((Y_i1-Y_i)/P(1,1))).*Y_i);
C_my(10,1)=sum((C_p10.*((X_i1-X_i)/P(1,1))).*(X_i-.5));

% alpha11
C_mx(11,1)=sum((C_p11.*((Y_i1-Y_i)/P(1,1))).*Y_i);
C_my(11,1)=sum((C_p11.*((X_i1-X_i)/P(1,1))).*(X_i-.5));

% alpha12
C_mx(12,1)=sum((C_p12.*((Y_i1-Y_i)/P(1,1))).*Y_i);
C_my(12,1)=sum((C_p12.*((X_i1-X_i)/P(1,1))).*(X_i-.5));

% alpha13
C_mx(13,1)=sum((C_p13.*((Y_i1-Y_i)/P(1,1))).*Y_i);
C_my(13,1)=sum((C_p13.*((X_i1-X_i)/P(1,1))).*(X_i-.5));

% alpha14
C_mx(14,1)=sum((C_p14.*((Y_i1-Y_i)/P(1,1))).*Y_i);
C_my(14,1)=sum((C_p14.*((X_i1-X_i)/P(1,1))).*(X_i-.5));

% alpha15
C_mx(15,1)=sum((C_p15.*((Y_i1-Y_i)/P(1,1))).*Y_i);
C_my(15,1)=sum((C_p15.*((X_i1-X_i)/P(1,1))).*(X_i-.5));

% alpha16
C_mx(16,1)=sum((C_p16.*((Y_i1-Y_i)/P(1,1))).*Y_i);
C_my(16,1)=sum((C_p16.*((X_i1-X_i)/P(1,1))).*(X_i-.5));

% alpha17
C_mx(17,1)=sum((C_p17.*((Y_i1-Y_i)/P(1,1))).*Y_i);
C_my(17,1)=sum((C_p17.*((X_i1-X_i)/P(1,1))).*(X_i-.5));

% alpha18
C_mx(18,1)=sum((C_p18.*((Y_i1-Y_i)/P(1,1))).*Y_i);
C_my(18,1)=sum((C_p18.*((X_i1-X_i)/P(1,1))).*(X_i-.5));

% alpha19
C_mx(19,1)=sum((C_p19.*((Y_i1-Y_i)/P(1,1))).*Y_i);
C_my(19,1)=sum((C_p19.*((X_i1-X_i)/P(1,1))).*(X_i-.5));

% alpha20
C_mx(20,1)=sum((C_p20.*((Y_i1-Y_i)/P(1,1))).*Y_i);
C_my(20,1)=sum((C_p20.*((X_i1-X_i)/P(1,1))).*(X_i-.5));

% alpha21
C_mx(21,1)=sum((C_p21.*((Y_i1-Y_i)/P(1,1))).*Y_i);
C_my(21,1)=sum((C_p21.*((X_i1-X_i)/P(1,1))).*(X_i-.5));

% alpha22
C_mx(22,1)=sum((C_p22.*((Y_i1-Y_i)/P(1,1))).*Y_i);
C_my(22,1)=sum((C_p22.*((X_i1-X_i)/P(1,1))).*(X_i-.5));

% alpha23
C_mx(23,1)=sum((C_p23.*((Y_i1-Y_i)/P(1,1))).*Y_i);
C_my(23,1)=sum((C_p23.*((X_i1-X_i)/P(1,1))).*(X_i-.5));

% alpha24
C_mx(24,1)=sum((C_p24.*((Y_i1-Y_i)/P(1,1))).*Y_i);
C_my(24,1)=sum((C_p24.*((X_i1-X_i)/P(1,1))).*(X_i-.5));

% alpha25
C_mx(25,1)=sum((C_p25.*((Y_i1-Y_i)/P(1,1))).*Y_i);
C_my(25,1)=sum((C_p25.*((X_i1-X_i)/P(1,1))).*(X_i-.5));

% alpha26
C_mx(26,1)=sum((C_p26.*((Y_i1-Y_i)/P(1,1))).*Y_i);
C_my(26,1)=sum((C_p26.*((X_i1-X_i)/P(1,1))).*(X_i-.5));

% 12.5 COEFICIENTE DE MOMENTO
C_m= C_mx + C_my


% 2.6 RAZÃO C_l/C_d
L_D=C_l./C_d


